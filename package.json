{
  "name": "cnmat-ws",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider preact build",
    "build:do": "yarn install --production=false && yarn build && rm -rf node_modules && yarn install --production --frozen-lockfile && cp static/* build",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider preact watch",
    "lint": "eslint src",
    "test": "jest",
    "start": "node server.js"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^4.0.0",
    "eslint": "^8.30.0",
    "eslint-config-preact": "^1.3.0",
    "jest-preset-preact": "^4.0.5",
    "preact-cli": "^3.4.2",
    "sirv-cli": "^2.0.2",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "express": "^4.18.2",
    "preact": "^10.11.3",
    "preact-render-to-string": "^5.2.6",
    "preact-router": "^3.2.1",
    "react-piano": "^3.1.3",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
